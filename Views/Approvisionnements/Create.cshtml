@model CheaperMarket.Models.Approvisionnement
@{
   
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Create", "Approvisionnements", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>Nouvel approvisonnement.</h4>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group row">
        @Html.LabelFor(m => m.quantite, new { @class = "col-md-4 control-label" })
        <div class="col-md-8">
            @Html.TextBoxFor(m => m.quantite, new { @class = "form-control  col-md-8" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-push-9 col-md-3">
            <input type="submit" style="margin-right:-10px;" class="btn btn-default" value="Enregistrer" />
        </div>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}